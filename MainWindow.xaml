<Window x:Class="AdoCurcus.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AdoCurcus"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <Label Content="RekeningNr.:" Margin="20 20 20 0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <TextBox Name="textBixRekeningNr" Margin="20 20 20 0"/>
        <Label Content="Te storten:" Margin="20 20 20 0"/>
        <TextBox Name="textBoxTeStorten" Margin="20 20 20 0"/>
        <Button Name="buttonStorten" Content="Storten" Margin="20 20 20 0" Click="buttonStorten_Click"/>
        <Label Name="labelStatus" Content="" Margin="20 20 0 0"
               HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </StackPanel>
</Window>
